create table wp_user_tracking(
username varchar(20), 	
date_entered_study date,
six_week_date date,
days_since_registered integer,	
total_num_of_log_ins_1st_6_weeks integer default 0,	
total_num_of_log_ins_ever integer default 0,	
mean_num_of_log_ins_per_week decimal(5,2) default 0,	
total_num_of_BP_views_1st_6_weeks integer default 0,	
total_num_of_BP_views_ever integer default 0,	
mean_num_of_BP_views_per_week decimal(5,2) default 0,	
total_num_of_EBP_views_1st_6_weeks integer default 0,	
total_num_of_EBP_views_ever integer default 0,	
mean_num_of_EBP_views_per_week decimal(5,2) default 0,	
total_num_of_SBP_views_1st_6_weeks integer default 0,	
total_num_of_SBP_views_ever integer default 0,	
mean_num_of_SBP_views_per_week decimal(5,2) default 0,	
total_num_of_PBP_views_1st_6_weeks integer default 0,	
total_num_of_PBP_views_ever integer default 0,	
mean_num_of_PBP_views_per_week decimal(5,2) default 0,	
total_num_of_LBP_views_1st_6_weeks integer default 0,	
total_num_of_LBP_views_ever integer default 0,	
mean_num_of_LBP_views_per_week decimal(5,2) default 0);
